<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">



    <LinearLayout
        android:id="@+id/middle_linearlayout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">
<!--        여기가 가운데 프레임-->
<!--        버튼 2개를 포함하고 있는 linealayout-->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <Button
                android:id="@+id/exercise_frag_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="10dp"
                android:text="exercise"
                android:background="@drawable/round_square_diet_exercise_white"
                />

            <Button
                android:id="@+id/diet_frag_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_margin="10dp"
                android:text="diet"
                android:background="@drawable/round_square_diet_exercise_white"
                />

        </LinearLayout>
        <FrameLayout
            android:id="@+id/exercise_diet_frame"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>



    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
